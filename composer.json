{
  "name": "matiux/ddd-starter-pack",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "ddd",
    "domain-driven-design",
    "domain",
    "message",
    "aggregate",
    "message publisher"
  ],
  "description": "Basic Domain Driven Design utils",
  "require": {
    "ext-json": "*",
    "php": ">=7.2",
    "ramsey/uuid": "^3.7|^4.0",
    "symfony/options-resolver": "^5.0 | ^4.0"
  },
  "require-dev": {
    "symfony/orm-pack": "^1.0",
    "aws/aws-sdk-php": "^3.134",
    "phpunit/phpunit": "^8",
    "mockery/mockery": "^1.3",
    "vimeo/psalm": "^4.0",
    "psalm/plugin-phpunit": "^0.13.0",
    "weirdan/doctrine-psalm-plugin": "dev-allow-psalm-4",
    "psalm/plugin-mockery": "^0.6.1",
    "friendsofphp/php-cs-fixer": "^2.16",
    "doctrine/orm": "^2.7"
  },
  "authors": [
    {
      "name": "Matteo Galacci",
      "email": "m.galacci@gmail.com",
      "homepage": "http://www.matteogalacci.it"
    }
  ],
  "autoload": {
    "psr-4": {
      "DDDStarterPack\\": "src/Matiux/DDDStarterPack"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\DDDStarterPack\\": "tests/DDDStarterPack/",
      "Tests\\Learning\\": "tests/Learning/",
      "Tests\\Support\\": "tests/Support/",
      "Tests\\Tool\\": "tests/Tool/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:tugmaks/doctrine-psalm-plugin.git"
    }
  ]
}
