{
  "name": "matiux/ddd-starter-pack",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "message",
    "message bus",
    "event bus",
    "command bus",
    "message publisher"
  ],
  "description": "Basic DDD structure",
  "require": {
    "php": ">=7.1",
    "ramsey/uuid": "^3.7",
    "jms/serializer-bundle": "^2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.4",
    "mockery/mockery": "^1.0"
  },
  "authors": [
    {
      "name": "Matteo Galacci",
      "email": "m.galacci@gmail.com",
      "homepage": "http://www.matteogalacci.it"
    }
  ],
  "autoload": {
    "psr-4": {
      "DDDStarterPack\\": "src/Matiux/DDDStarterPack"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\DDDStarterPack\\": "tests"
    }
  },
  "config": {
    "platform": {
      "php": "7.1.8"
    }
  }
}
